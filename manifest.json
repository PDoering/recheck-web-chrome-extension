{
	"manifest_version": 2,
	"name": "recheck-web",
	"description": "Click to recheck this website",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"https://*/*",
				"http://*/*",
				"file:///*"
			],
			"js": [
				"getAllElementsByPath.js",
				"content.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "icon.png"
	},
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": true
	},
	"web_accessible_resources": [
		"login.html",
		"login.js",
		"keycloak.min.js"
	]
}

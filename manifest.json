{
	"manifest_version": 2,
	"name": "recheck-web",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"https://*/*",
				"http://*/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "icon.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	}
}
